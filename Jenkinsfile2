
pipeline {
    agent any
    stages {
        stage('my_stage'){
            steps{
                script{
                    withPythonEnv('python'){
                        sh 'pip install --upgrade pip'
                        sh 'pip install --user -r requirements.txt'
                        sh 'python dashboard1.py' 
                    }
                }
            }
        }
    }
}
